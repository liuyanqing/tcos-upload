{
	"name": "tcos-upload",
	"version": "1.0.3",
	"description": "上传静态资源到腾讯云 COS 的 Nodejs 插件",
	"main": "lib/index.cjs",
	"module": "index.js",
	"types": "lib/index.d.ts",
	"type": "module",
	"files": [
		"lib",
		"index.js"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/liuyanqing/tcos-upload.git"
	},
	"keywords": [
		"腾讯云",
		"文件上传",
		"file",
		"cos",
		"upload"
	],
	"scripts": {
		"prepare-husky": "husky install && chmod u+x .husky/pre-commit && chmod u+x .husky/commit-msg",
		"demo": "node demo/upload.js",
		"build": "rm -rf dist && rollup index.js --file dist/index.cjs --format cjs",
		"prepublishOnly": "pnpm run build && pnpm run types",
		"cz": "git-cz",
		"version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
		"tsc": "tsc"
	},
	"author": "auroraliu <auroralyqing@gmail.com>",
	"license": "ISC",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": "eslint --fix"
	},
	"dependencies": {
		"chalk": "^3.0.0",
		"commander": "^10.0.1",
		"cos-nodejs-sdk-v5": "^2.12.1",
		"fs-extra": "^11.1.1",
		"minimist": "^1.2.8",
		"rollup": "^3.23.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.1",
		"conventional-changelog-cli": "^2.2.2",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.40.0",
		"git-cz": "^4.9.0",
		"typescript": "^5.0.4"
	}
}
