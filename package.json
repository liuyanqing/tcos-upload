{
	"name": "tcos-upload",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
  "types": "index.d.ts",
	"type": "module",
	"scripts": {
		"postinstall": "husky install && chmod u+x .husky/pre-commit && chmod u+x .husky/commit-msg",
		"demo": "node demo/upload.js",
		"cz": "git-cz",
		"version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
		"tsc": "tsc"
	},
	"author": "",
	"license": "ISC",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": "eslint --fix"
	},
	"dependencies": {
		"chalk": "^5.2.0",
		"commander": "^10.0.1",
		"cos-nodejs-sdk-v5": "^2.12.1",
		"fs-extra": "^11.1.1",
		"inquirer": "^9.2.3",
		"minimist": "^1.2.8",
		"typescript": "^5.0.4"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.1",
		"conventional-changelog-cli": "^2.2.2",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.40.0",
		"git-cz": "^4.9.0"
	}
}
